// ActiveIndex Debug
console.log("ActiveIndex: " + ActiveIndex)

var image_url = '/e/img/renew/common/';

var menuItem = new Array();
menuItem[1] = '#menuPass';
menuItem[2] = '#menuRoute';
menuItem[3] = '#menuCustomer';
menuItem[4] = '#menuDestinations';
menuItem[5] = '#menuInvestor';
menuItem[6] = '#menuAboutus';

function ShowActiveMenu() {
  if (ActiveIndex >= 0) {
    var ele = $(menuItem[ActiveIndex] + ' > a > img');
    ele.attr('src',
      image_url + 'menu0' + (ActiveIndex) + '_active.png'
    );
  }
}

function HideAllMenu(menuName) {
  var menuList = new Array(7);
  for (n = 1; n <= 6; n++) {
    var ele = $(menuItem[n] + ' > a > img');
    ele.attr('src', image_url + 'menu0' + (n) + '.png');
  }
  ShowActiveMenu();
}

function ViewMenu(menuName) {
  HideAllMenu(menuName);
  var ele = $(menuItem[menuName] + ' > a > img');
  var tmp = image_url + 'menu0' + (menuName) + '_active.png';
  ele.attr('src', tmp);
}

function OverMenu(menuName) {
  if (!$(menuItem[menuName] + ' > a').hasClass('active')) {
    var ele = $(menuItem[menuName] + ' > a > img');
    var tmp = image_url + 'menu0' + (menuName) + '_on.png';
    if (ActiveIndex != menuName)
      ele.attr('src', tmp);
  }
}

function OutMenu(menuName) {
  if (!$(menuItem[menuName] + ' > a').hasClass('active')) {
    var ele = $(menuItem[menuName] + ' > a > img');
    var tmp = image_url + 'menu0' + (menuName) + '.png';
    if (ActiveIndex != menuName)
      ele.attr('src', tmp);
  }
}

$("#menuPass").mouseover(function () {
  OverMenu(1);
});
$("#menuRoute").mouseover(function () {
  OverMenu(2);
});
$("#menuCustomer").mouseover(function () {
  OverMenu(3);
});
$("#menuDestinations").mouseover(function () {
  OverMenu(4);
});
$("#menuInvestor").mouseover(function () {
  OverMenu(5);
});
$("#menuAboutus").mouseover(function () {
  OverMenu(6);
});

$("#menuPass").mouseout(function () {
  OutMenu(1);
});
$("#menuRoute").mouseout(function () {
  OutMenu(2);
});
$("#menuCustomer").mouseout(function () {
  OutMenu(3);
});
$("#menuDestinations").mouseout(function () {
  OutMenu(4);
});
$("#menuInvestor").mouseout(function () {
  OutMenu(5);
});
$("#menuAboutus").mouseout(function () {
  OutMenu(6);
});

$("#menuPass").click(function () {
  ViewMenu(1);
});
$("#menuRoute").click(function () {
  ViewMenu(2);
});
$("#menuCustomer").click(function () {
  ViewMenu(3);
});
$("#menuDestinations").click(function () {
  ViewMenu(4);
});
$("#menuInvestor").click(function () {
  ViewMenu(5);
});
$("#menuAboutus").click(function () {
  ViewMenu(6);
});
$(".close").click(function () {
  HideAllMenu();
})

ShowActiveMenu();
