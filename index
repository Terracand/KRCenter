		<!-- Emergency3 -->
		<section id="emergency3">
			<div class="emg-inner">
				<!-- アコーディオン使用時はdlに「js-accordion」クラスを設定しdtを表示 -->
				<dl class="emg-item">
<!--				<dt>Important Notices</dt>-->
					<dd>
