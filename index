	<!-- What's New and Notice form JR East -->
	<section id="whatsnew-area">
		<div class="rwdInner clearfix">
			<!-- Left Area -->
			<div class="leftArea">
			<!-- What's New -->
			<script type="text/javascript">
			window.addEventListener('wovnLangChanged', function(evt) {
				const whatsNewList = document.querySelectorAll('.whatsnewContent > li > dl');
				const noticesList = document.querySelectorAll('.whatsnewContent > li .newsList');
				for (let i = 0; i < whatsNewList.length; i++) {
					while(whatsNewList[i].lastChild) {
						whatsNewList[i].removeChild(whatsNewList[i].lastChild);
					}
				}
				for (let i = 0; i < noticesList.length; i++) {
					while(noticesList[i].lastChild) {
						noticesList[i].removeChild(noticesList[i].lastChild);
					}
				}
				/* 英語 en */
				if(document.documentElement.lang == 'en'){
					readJSON("/e/json/en_whatsnew.json", false, 10);
				/* 繁体字 tc(zh-Hant) */
				} else if(document.documentElement.lang == 'zh-Hant') {
					readJSON("/tc/json/zh-tw_whatsnew.json", false, 10);
				/* 簡体字 sc(zh-Hans) */
				} else if(document.documentElement.lang == 'zh-Hans'){
					readJSON("/sc/json/zh-cn_whatsnew.json", false, 10);
				/* 韓国語 kr(ko) */
				} else if(document.documentElement.lang == 'ko'){
					readJSON("/kr/json/ko_whatsnew.json", false, 10);
				/* その他の言語の時は英語 */
				} else {
					readJSON("/e/json/en_whatsnew.json", false, 10);
				}
			});
			</script>
